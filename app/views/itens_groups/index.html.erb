<p id="notice"><%= notice %></p>

<h1>Itens Groups</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @itens_groups.each do |itens_group| %>
      <tr>
        <td><%= itens_group.product %></td>
        <td><%= itens_group.cart %></td>
        <td><%= link_to 'Show', itens_group %></td>
        <td><%= link_to 'Edit', edit_itens_group_path(itens_group) %></td>
        <td><%= link_to 'Destroy', itens_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Itens Group', new_itens_group_path %>
